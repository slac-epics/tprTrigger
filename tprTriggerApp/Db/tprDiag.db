record(bo, "$(DEV):MODE")
{
    field(DESC, "Mode selection NC/SC")
    field(DTYP, "asynInt32")
    field(OUT,  "@asyn($(PORT),0) mode")
    field(OMSL, "supervisory")
    field(ZNAM, "NC")
    field(ONAM, "SC")
    field(PINI, "YES")
    info(autosaveFields, "VAL ZSV OSV")
}

record(ao, "$(DEV):MSGDLY")
{
    field(DESC, "Message Delay for LCLS2 Timing Frame")
    field(DTYP, "asynFloat64")
    field(OUT,  "@asyn($(PORT),0) msgDelay")
    field(OMSL, "supervisory")
    field(EGU,  "nsec")
    field(VAL, "107692.3076923077")
    field(PREC, "0")
    field(HOPR, "$(DLYLIMIT=)")
    field(DRVH, "$(DLYLIMIT=)")
    field(PINI, "YES")
    info(autosaveFields, "VAL")
}

