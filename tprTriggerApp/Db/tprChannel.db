record(calcout, "$(DEV):CH$(NN)_CALC") {
  field(DESC, "Calculate TimeSlots")
  field(CALC, "(A*1)OR(B*2)OR(C*4)OR(D*8)OR(E*16)OR(F*32)")
  field(INPA, "$(DEV):CH$(NN)_TS1")
  field(INPB, "$(DEV):CH$(NN)_TS2")
  field(INPC, "$(DEV):CH$(NN)_TS3")
  field(INPD, "$(DEV):CH$(NN)_TS4")
  field(INPE, "$(DEV):CH$(NN)_TS5")
  field(INPF, "$(DEV):CH$(NN)_TS6")
  field(OUT, "$(DEV):CH$(NN)_TSMASK PP")
}

